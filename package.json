{
  "name": "webpack-basic",
  "version": "0.0.1",
  "description": "webpack应用",
  "main": "index.js",
  "author": "kiscon",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --info=false --config build/webpack.dev.js",
    "dev:uat": "PROXY_ENV=uat yarn dev",
    "build": "webpack --config build/webpack.prod.js",
    "build:dev": "webpack --config build/webpack.dev.js",
    "dll": "rm -rf dll/ && webpack --config build/webpack.dll.js",
    "analyse": "webpack --profile --json > stats.json --config build/webpack.analyse.js",
    "watch": "webpack --watch",
    "server": "node server.js",
    "start": "live-server ./dist"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1"
  }
}
