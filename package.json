{
  "name": "webpack-basic",
  "version": "0.0.1",
  "description": "webpack应用",
  "main": "index.js",
  "author": "kiscon",
  "license": "ISC",
  "scripts": {
    "dev": "yarn path webpack-dev-server --inline --progress --config build/webpack.dev.js",
    "dev:uat": "PROXY_ENV=uat yarn dev",
    "build": "yarn path webpack --config build/webpack.prod.js",
    "dll": "rm -rf dll/ && webpack --config build/webpack.dll.js",
    "build:analyse": "webpack --profile --json > stats.json --config build/webpack.analyse.js",
    "build:smp": "NODE_ENV=smp yarn path webpack --config build/webpack.smp.js",
    "lint": "eslint --ext .js,.vue src --cache",
    "watch": "webpack --watch",
    "start": "node build/start/index.js",
    "path": "PROJECT_PATH=test"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "vue": "^2.6.11",
    "vue-router": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@vue/eslint-config-standard": "^5.1.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.1.2",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "portfinder": "^1.0.25",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "speed-measure-webpack-plugin": "^1.3.1",
    "style-loader": "^1.1.2",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^3.0.0",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}
