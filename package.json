{
  "name": "webpack-basic",
  "version": "2.0.2",
  "description": "webpack5+应用",
  "author": "kiscon",
  "license": "ISC",
  "scripts": {
    "dev": "webpack-dev-server --progress --color --config build/webpack.dev.js",
    "dev:uat": "cross-env PROXY_ENV=uat yarn dev",
    "build": "webpack --config build/webpack.prod.js",
    "build:sz": "node --max_old_space_size=3072 build/build.js",
    "dll": "rm -rf dll/ && webpack --config build/webpack.dll.js",
    "analyse": "webpack --profile --json > stats.json --config build/webpack.analyse.js",
    "smp": "cross-env NODE_ENV=smp webpack --config build/webpack.smp.js",
    "lint": "eslint --ext .js,.vue src --cache",
    "watch": "webpack --watch",
    "start": "node start",
    "path": "cross-env PROJECT_PATH=todo1"
  },
  "dependencies": {
    "vue": "^2.6.14",
    "vue-router": "^3.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "@vue/eslint-config-standard": "^6.1.0",
    "add-asset-html-webpack-plugin": "^3.2.0",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^7.20.0",
    "fast-sass-loader": "^2.0.0",
    "file-loader": "^6.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^5.5.0",
    "html-withimg-loader": "^0.1.16",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "node-sass": "^6.0.1",
    "portfinder": "^1.0.28",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.8",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.60.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  }
}
